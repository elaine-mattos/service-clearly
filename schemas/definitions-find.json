    },
    "maxLicensedScore": {
      "type": "number",
      "errorMessage": {
        "type": "maxLicensedScore must be a number"
      }
    },
    "minDescribedScore": {
      "type": "number",
      "errorMessage": {
        "type": "minDescribedScore must be a number"
      }
    },
    "maxDescribedScore": {
      "type": "number",
      "errorMessage": {
        "type": "maxDescribedScore must be a number"
      }
