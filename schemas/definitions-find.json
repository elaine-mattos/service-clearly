      "type": "string",
      "errorMessage": {
        "type": "type must be a string"
      }
    },
    "provider": {
      "type": "string",
      "errorMessage": {
        "type": "provider must be a string"
      }
    },
    "name": {
      "type": "string",
      "errorMessage": {
        "type": "name must be a string"
      }
    },
    "namespace": {
      "type": "string",
      "errorMessage": {
        "type": "namespace must be a string"
      }
    },
    "license": {
      "type": "string",
      "errorMessage": {
        "type": "license must be a string"
      }
    },
    "releasedAfter": {
      "type": "string",
      "errorMessage": {
        "type": "releasedAfter must be a string"
      }
    },
    "releasedBefore": {
      "type": "string",
      "errorMessage": {
        "type": "releasedBefore must be a string"
      }
